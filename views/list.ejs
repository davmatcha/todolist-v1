<%- include("header") -%> 


    <div class="box" id="heading">
    <h1><%= listTitle %></h1>
    </div>
    <div class="box">
    <% for( let i = 0; i < newListItems.length; i++ ) { %>
        <div class="item">
            <input type="checkbox" name="" value="">
        <p><%= newListItems[i] %> </p>
        </div>
    <% } %>
    <form class="item"action="/" method="POST" name="">
        <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
        <button type="submit" value=<%= listTitle %> name="list">+</button>
    </form>
    </div>
<%- include("footer") -%> 